<Wizard>
	<Global>

		<CustomStatement><![CDATA[ document.title =  "KSGR Deployment Wizard" ]]> </CustomStatement>

		<Initialization>DeployWiz_Initialization.vbs</Initialization>		
		<Validation>DeployWiz_Validation.vbs</Validation>

	</Global>

	<!-- ************************************************************** -->
	<!-- ************************************************************** -->

	
	<Pane id="RequestSelection" reference="DeployWiz_RequestSelection.xml">
		<Condition><![CDATA[ ucase(oEnvironment.Item("KUO_SkipRequestSelection")) <> "YES" ]]></Condition>
	</Pane>

	
	<Pane id="ComputerConfiguration" reference="DeployWiz_ComputerConfiguration.xml">
		<Condition><![CDATA[ ucase(oEnvironment.Item("KUO_SkipComputerConfiguration")) <> "YES" ]]></Condition>
	</Pane>
	
	<Pane id="Summary" reference="DeployWiz_Summary.xml">
		<Condition><![CDATA[ ucase(oEnvironment.Item("KUO_SkipSummary")) <> "YES" ]]></Condition>
	</Pane>


</Wizard>
